    agent any
    stages {
        stage('Init') {
            steps {
                echo 'Hi, this is Malchiel learning about Jenkins'
                echo 'Hi, here again'
            }
        }
        stage('Build') {
            steps {
                echo 'Building Sample Maven Project'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying in staging'
            }
        }
    }
}